<SpacewarConfiguration>
	<!-- This file is probably no longer valid; however, we should keep it 
		 around for now as reference for a future similar config. -->

    <!--Specify the random seed (used for replay abilities)-->
    <seed>1</seed>

    <!--Specify the dimensions of the SpacewarGame-->
    <width>800.0</width>
    <height>600.0</height>

    <!--Specify the max initial starting speed for any ships or obstacles-->
    <maxInitialSpeed>30.0</maxInitialSpeed>

    <!--Specify the number of teams that will be in the environment-->
    <numberOfTeams>2</numberOfTeams>

    <!--Manually specify the parameters of each team-->
    <teams>
        <TeamInformation>
            <name>Red</name>
            <number>1</number>
            <agentClass>OurCentralCommand</agentClass>
            <agentRecord>true</agentRecord>
        </TeamInformation>
        <TeamInformation>
            <name>Blue</name>
            <number>2</number>
            <agentClass>OurCentralCommand</agentClass>
            <agentRecord>true</agentRecord>
        </TeamInformation>
    </teams>

    <!--Specify the number of ships that will be in the environment,
        as well as the default Agent that should control any ships that
        aren't manually specified -->
    <numberOfShips>6</numberOfShips>
    <defaultShipAgent>AIClass.ai.agents.SWDoNothingAgent</defaultShipAgent>

    <!--Manually specify any specific ships that are wanted in the environment.
        Note that any fields left unitialized will default to 0.  An agentClass
        must be specified for each manually added ship.  Also, specify whether
        or not the agent's data should be recorded (if recording is available).

        If fewer than numberOfShips ships are specified, then the
        remaining ships will be filled with instances of the default agent
        specified above, randomly placed under the constraints
        given by preferredBuffer and minBuffer-->
    <ships>
        <ShipInformation>
            <name>Red1</name>
            <team>1</team>
            <positionX>300.0</positionX>
            <positionY>250.0</positionY>
            <orientedX>-1.0</orientedX>
            <orientedY>0.0</orientedY>
            <agentClass>OurSpacewarAgent</agentClass>
            <agentRecord>false</agentRecord>
        </ShipInformation>
        <ShipInformation>
            <name>Red2</name>
            <team>1</team>
            <positionX>300.0</positionX>
            <positionY>300.0</positionY>
            <orientedX>-1.0</orientedX>
            <orientedY>0.0</orientedY>
            <agentClass>OurSpacewarAgent</agentClass>
            <agentRecord>false</agentRecord>
        </ShipInformation>
        <ShipInformation>
            <name>Red3</name>
            <team>1</team>
            <positionX>300.0</positionX>
            <positionY>350.0</positionY>
            <orientedX>-1.0</orientedX>
            <orientedY>0.0</orientedY>
            <agentClass>OurSpacewarAgent</agentClass>
            <agentRecord>false</agentRecord>
        </ShipInformation>
        <ShipInformation>
